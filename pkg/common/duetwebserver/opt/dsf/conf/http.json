{
  "Logging": {
    "LogLevel": {
      "Default": "Information"
    }
  },
  "Kestrel": {
    "Endpoints": {
      "Http": {
        "Url": "http://*"
      }
    }
  },
  "KeepAliveInterval": 30,
  "ModelRetryDelay": 5000,
  "UseStaticFiles": true,
  "SocketPath": "/var/run/dsf/dcs.sock",
  "WebSocketBufferSize": 8192
}